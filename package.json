{
  "name": "pathern",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "22.2.2",
    "jest": "22.4.3",
    "ts-jest": "22.4.2",
    "typescript": "2.8.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.test.{ts,tsx}",
      "!**/*.d.ts",
      "!(coverage|node_modules)/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "\\.test\\.(ts|tsx|js|jsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
